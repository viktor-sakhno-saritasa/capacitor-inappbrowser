{
  "name": "@sakhnovict/inappbrowser",
  "version": "7.0.0",
  "description": "Capacitor In-App Browser plugin for the Pager project",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "SakhnovictInappbrowser.podspec"
  ],
  "author": "Martin Donadieu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cap-go/capacitor-inappbrowser.git"
  },
  "bugs": {
    "url": "https://github.com/Cap-go/capacitor-inappbrowser/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "browser",
    "native"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "docgen": "docgen --api InAppBrowserPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^7.0.0",
    "husky": "^8.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
